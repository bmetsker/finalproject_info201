server <- function(input, output) {
output$plot <- renderPlotly({
plot_ly(
data = midwest,
x = midwest[[input$x1]],
y = midwest[[input$y1]],
type = "scatter",
alpha = .7,
hovertext = "y"
) %>%
layout(
title = "Poverty by county",
xaxis = list(title = "county"),
yaxis = list(title = "poverty")
)
})
output$chart <- renderPlotly({
plot_ly(
data = midwest,
x = midwest[[input$x2]],
y = midwest[[input$y2]],
type = "scatter",
alpha = .7,
hovertext = "y"
) %>%
layout(
title = "Education by race",
xaxis = list(title = "education"),
yaxis = list(title = "race")
)
})
}
